{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    
    "java.dependency.packagePresentation": "flat",
    "java.configuration.updateBuildConfiguration": "automatic",

    "workbench.colorTheme": "Lime Theme",
    "discord.workspaceElapsedTime": true,

    "files.associations": {
        "*.html": "twig"
    },

    "emmet.includeLanguages": {
        "twig": "html"
    },
}